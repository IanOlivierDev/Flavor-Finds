<% if (success.length > 0) { %>
    <div class="alert alert-success alert-dismissible fade show custom-alert" role="alert" role="alert">
        <%= success%>
        <button type="button" class="close" type="button" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
<% } %>